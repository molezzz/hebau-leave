<p id="notice"><%= notice %></p>

<h1>Records</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Approver</th>
      <th>Status</th>
      <th>Begin at</th>
      <th>End at</th>
      <th>Address</th>
      <th>Tel</th>
      <th>Cause</th>
      <th>Agent</th>
      <th>Agent office</th>
      <th>Agent office tel</th>
      <th>Agent mobile</th>
      <th>Back at</th>
      <th>Exdata</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @records.each do |record| %>
      <tr>
        <td><%= record.user_id %></td>
        <td><%= record.approver_id %></td>
        <td><%= record.status %></td>
        <td><%= record.begin_at %></td>
        <td><%= record.end_at %></td>
        <td><%= record.address %></td>
        <td><%= record.tel %></td>
        <td><%= record.cause %></td>
        <td><%= record.agent %></td>
        <td><%= record.agent_office %></td>
        <td><%= record.agent_office_tel %></td>
        <td><%= record.agent_mobile %></td>
        <td><%= record.back_at %></td>
        <td><%= record.exdata %></td>
        <td><%= link_to 'Show', record %></td>
        <td><%= link_to 'Edit', edit_record_path(record) %></td>
        <td><%= link_to 'Destroy', record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Record', new_record_path %>
